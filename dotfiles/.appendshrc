
export LC_CTYPE=en_US.UTF-8
export LC_ALL=en_US.UTF-8

export EDITOR="nvim"
export PATH="$PATH:/usr/local/node/bin:$HOME/.yarn/bin:$HOME/.local/bin:$HOME/.cargo/bin:$HOME/.config/composer/vendor/bin:$HOME/go/bin:/home/linuxbrew/.linuxbrew/bin"
export PROXY="http://127.0.0.1:7890"
export GHSSH="ssh://git@ssh.github.com:443"
export GHPROXY1="https://gitclone.com/github.com"
export GHPROXY2="https://kkgithub.com"
export GHPROXY3="https://gitdl.cn/https://github.com"
export GHPROXY4="https://mirror.ghproxy.com/https://github.com"
export GHPROXY5="https://githubfast.com"
export GHPROXY6="https://ghproxy.net/https://github.com"

export GO111MODULE=on
export GOPROXY=https://goproxy.cn

export ATAC_KEY_BINDINGS="$HOME/.config/custom/atac_vim_key_binding.toml"

# aliases
[[ -f $HOME/init/dotfiles/.alias ]] && source $HOME/init/dotfiles/.alias

