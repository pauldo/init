export EDITOR="nvim"
export PATH="$PATH:/usr/local/node/bin:$HOME/.yarn/bin:$HOME/.local/bin:$HOME/.cargo/bin:$HOME/.config/composer/vendor/bin:$HOME/.composer/vendor/bin:$HOME/go/bin:$HOME/.venv/bin:/home/linuxbrew/.linuxbrew/bin"

export JAVA_HOME=/opt/homebrew/opt/openjdk

export UNAMEOUT="$(uname -s)"
function ts {
  case ${UNAMEOUT} in
      Linux*)     date +%s;;
      Darwin*)    date -j -f "%a %b %d %T %Z %Y" "`date`" "+%s";;
  esac
}
function dt {
  case ${UNAMEOUT} in
      Linux*)     date +'%Y-%m-%d %H:%M:%S';;
      Darwin*)    date -j -f "%a %b %d %T %Z %Y" "`date`" "+%Y-%m-%d %H:%M:%S";;
  esac
}
function ts2dt {
  case ${UNAMEOUT} in
      Linux*)     date -d "@$1" +'%Y-%m-%d %H:%M:%S';;
      Darwin*)    date -j -f "%s" "$1" "+%Y-%m-%d %H:%M:%S";;
  esac
}
function dt2ts {
  case ${UNAMEOUT} in
      Linux*)     date -d "$1" +'%s';;
      Darwin*)    date -j -f "%Y-%m-%d %H:%M:%S" "$1" "+%s";;
  esac
}
